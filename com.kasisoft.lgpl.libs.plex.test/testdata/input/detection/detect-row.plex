<?xml version="1.0" encoding="UTF-8"?>
<plex>

  <general>
    <interface api="transform" id="cleanup"         classname="com.kasisoft.lgpl.libs.plex.impl.CleanupTransform"    />
    <interface api="row"       id="simpleresolver"  classname="com.kasisoft.lgpl.libs.plex.impl.SimpleRowResolver"   /> 
  </general>

  <sheet name="detect-row">
    <firstrowdetect refid="simpleresolver">
    </firstrowdetect>
    <column title="key"       column="e">
      <transformer refid="cleanup"      />
    </column>
    <column title="value"     column="f">
      <transformer refid="cleanup"      />
    </column>
  </sheet>
  
</plex>