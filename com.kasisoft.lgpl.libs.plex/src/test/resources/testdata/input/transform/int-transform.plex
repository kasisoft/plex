<?xml version="1.0" encoding="UTF-8"?>
<plex>

  <general>
    <interface api="transform" id="cleanup"   classname="com.kasisoft.lgpl.libs.plex.impl.CleanupTransform"             />
    <interface api="transform" id="enum"      classname="com.kasisoft.lgpl.libs.plex.test.TransformTest$EnumTransform"  />
    <interface api="transform" id="tolong"    classname="com.kasisoft.lgpl.libs.plex.impl.ToIntTransform">
      <boolean name="longType" value="true"/>
    </interface>
  </general>

  <sheet name="int-transform" firstrow="1">
    <column title="bug" column="a">
      <transformer refid="cleanup"      />
      <transformer refid="tolong"       />
    </column>
    <column title="state">
      <transformer refid="cleanup"      />
      <transformer refid="enum"         />
    </column>
  </sheet>
  
</plex>